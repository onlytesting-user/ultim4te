{
    "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
    "blocks": [
      {
        "alignment": "left",
        "segments": [
          {
            "foreground": "#ee5959",
            "properties": {
              "style": "folder"
            },
            "style": "plain",
            "template": "{{ .Path }}/ ",
            "type": "path"
          },
          {
            "foreground": "#f967bf",
            "properties": {
              "fetch_status": true
            },
            "style": "plain",
            "template": "<#ffffff>on</> {{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }} ",
            "type": "git"
          },
          {
            "foreground": "#84ffff",
            "properties": {
              "fetch_version": true
            },
            "style": "plain",
            "template": "<#ffffff>via</> \ue7ba {{ .Full }}",
            "type": "react"
          },
          {
            "foreground": "#8993BE",
            "properties": {
              "fetch_composer": true,
              "fetch_version": true
            },
            "style": "plain",
            "template": "<#ffffff>via</> \ue608 {{ .Full }} ",
            "type": "php"
          },
          {
            "foreground": "#6CC24A",
            "properties": {
              "fetch_package_manager": true,
              "fetch_version": true
            },
            "style": "plain",
            "template": "<#ffffff>via</> \ued0d {{ .Full }} ",
            "type": "node"
          },
          {
            "foreground": "#F09456",
            "properties": {
              "fetch_version": true
            },
            "style": "plain",
            "template": "<#ffffff>via</> \ue235 {{ .Full }} ",
            "type": "python"
          }
        ],
        "type": "prompt"
      },
      {
        "alignment": "right",
        "segments": [
          {
            "foreground": "#ffffff",
            "leading_diamond": "\ue641",
            "properties": {
              "style": "roundrock",
              "threshold": 0
            },
            "style": "diamond",
            "template": " {{ .FormattedMs }} ",
            "type": "executiontime"
          }
        ],
        "type": "prompt"
      },
      {
        "alignment": "left",
        "newline": true,
        "segments": [
          {
            "foreground": "#fff200",
            "style": "plain",
            "template": "\u2b9e ",
            "type": "text"
          }
        ],
        "type": "prompt"
      }
    ],
    "final_space": true,
    "version": 2
  }
