{
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "style": "folder"
          },
          "template": " \ueaf7 {{ .Path }}/ ",
          "foreground": "#50C878",
          "type": "path",
          "style": "plain"
        },
        {
          "properties": {
            "fetch_status": true
          },
          "template": "<#ffffff>on</> {{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }} ",
          "foreground": "#FF69B4",
          "type": "git",
          "style": "plain"
        },
        {
          "properties": {
            "fetch_version": true
          },
          "template": "<#ffffff>via</> \ue627 {{ .Full }} ",
          "foreground": "#00ADD8",
          "type": "go",
          "style": "plain"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "template": "\u279c ",
          "foreground": "#f1184c",
          "type": "text",
          "style": "plain"
        }
      ],
      "newline": true
    }
  ],
  "version": 3,
  "final_space": true
}
